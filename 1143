import java.util.Scanner;

import java.io.*;

public class Main{
	public static void main(String[] args){
        PrintStream sa = System.out;
        Scanner sc = new Scanner(System.in);
        int x;
        double a,b;
        x = sc.nextInt();
        for(int i=1;i<=x;i++){
            a = Math.pow(i,2);
            b = Math.pow(i,3);
            sa.printf("%d %.0f %.0f\n",i,a,b);
        }


	}
}
