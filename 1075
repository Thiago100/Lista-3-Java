import java.util.Scanner;

import java.io.*;

public class Main{
	public static void main(String[] args){
        PrintStream sa = System.out;
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<10000;i++){
            if(i%n==2) sa.println(i);
        }

	}
}
